Phi(
  (c): Linear(in_features=3, out_features=1, bias=True)
  (w): Linear(in_features=32, out_features=1, bias=False)
  (N): ResNN(
    (layers): ModuleList(
      (0): Linear(in_features=3, out_features=32, bias=True)
      (1): Linear(in_features=32, out_features=32, bias=True)
    )
  )
)
